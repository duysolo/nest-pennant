{
  "name": "@mercury-labs/nest-pennant",
  "version": "1.0.0",
  "description": "A simple and light-weight feature flag package for NestJS",
  "author": "duysolo <duypt.dev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duysolo/nest-pennant.git"
  },
  "keywords": [
    "mercury-labs",
    "feature flag",
    "features flag",
    "nestjs feature flag",
    "nestjs features flag"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "increase-version": "node scripts/increase-version.js",
    "test": "jest --verbose=true",
    "test:watch": "jest --verbose=true --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose=true --config ./test/jest-e2e.json",
    "test:all": "npm run test && npm run test:cov && npm run test:e2e"
  },
  "dependencies": {},
  "devDependencies": {
    "@nestjs/core": "^10.0.4",
    "@nestjs/common": "^10.0.4",
    "@nestjs/cqrs": "^10.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/cli": "^10.1.0",
    "@nestjs/platform-express": "^10.0.4",
    "@nestjs/platform-fastify": "^10.0.4",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.4",
    "@rushstack/eslint-config": "~3.3.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "rimraf": "^5.0.1"
  },
  "peerDependencies": {
    "@nestjs/core": "^10.0.0 || ^9.0.0 || ^8.0.0 || ^7.0.0",
    "@nestjs/common": "^10.0.0 || ^9.0.0 || ^8.0.0 || ^7.0.0",
    "@nestjs/cqrs": "^10.0.0 || ^9.0.0 || ^8.0.0 || ^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "peerDependenciesMeta": {},
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/index.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/duysolo/mercury-auth/issues"
  },
  "homepage": "https://github.com/duysolo/mercury-auth#readme",
  "directories": {
    "test": "test"
  }
}
